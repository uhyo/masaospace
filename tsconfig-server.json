{
  "compilerOptions":{
    "lib": ["dom", "es2015"],
    "alwaysStrict": true,
    "jsx":"React",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "removeComments": true,
    "module":"commonjs",
    "target":"es5",
    "outDir": "dist-server/"
  },
  "include": [
    "src/**/*.ts"
  ]
}
